{
  "name": "windcraft-ts-plugin",
  "version": "0.0.1",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./ast/*": "./dist/ast/*.js",
    "./client/*": "./dist/client/*.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "tsc -p . -w",
    "build": "rm -rf ./dist && tsc -p .",
    "check:types": "tsc -p . --noEmit"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/express": "^4.17.21"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "express": "^4.19.2",
    "zod": "^3.23.8"
  }
}
