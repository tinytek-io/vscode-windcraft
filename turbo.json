{
  "$schema": "https://turbo.build/schema.json",
  "remoteCache": {
    "enabled": false
  },
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "check:types": {
      "dependsOn": ["^check:types"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "clean": {
      "dependsOn": ["^clean"]
    }
  }
}
